    "@typescript-eslint/parser": "^2.6.1",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.3",
    "codecov": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
